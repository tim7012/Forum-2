<% unless @post.find_my_like(current_user) %>
  <%= link_to "Like", like_post_path(@post), :method => :post, :remote => true, :class => "btn btn-primary"%>
<%else%>
  <%= link_to "Dislike", dislike_post_path(@post), :method => :post, :remote => true, :class => "btn btn-primary"%>
<%end%> <%=@post.likes_by.count%>